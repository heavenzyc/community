<?xml version="1.0" encoding="UTF-8"?>
<json>
	<item name="resourceList">
		<param name="includeProperties">
			\[\d+\]\.id,
			\[\d+\]\.name,
			\[\d+\]\.path,
			\[\d+\]\.entryIndex,
			\[\d+\]\.description,
			\[\d+\]\.resourceGroup\.id,
			\[\d+\]\.resourceGroup\.name
		</param>
	</item>
	<item name="resourceGroupList">
		<param name="includeProperties">
			\[\d+\]\.id,
			\[\d+\]\.name,
			\[\d+\]\.resources,
			\[\d+\]\.resources\[\d+\]\.id,
			\[\d+\]\.resources\[\d+\]\.name,
			\[\d+\]\.resources\[\d+\]\.path,
			\[\d+\]\.resources\[\d+\]\.entryIndex,
			\[\d+\]\.resources\[\d+\]\.description
		</param>
	</item>
</json>